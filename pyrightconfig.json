{
    "venvPath": ".",
    "venv": ".venv",
    "executionEnvironments": [
        {
            "root": "backend",
            "pythonVersion": "3.13",
            "pythonPlatform": "Windows",
            "extraPaths": [
                ".venv/Lib/site-packages"
            ]
        }
    ],
    "include": [
        "backend/**/*.py"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.venv",
        "**/venv"
    ]
}
